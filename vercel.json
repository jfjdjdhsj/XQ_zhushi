{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10" 
    // Vercel 默认使用 Python 3.9，您可以根据需要更改
    // 例如 "PYTHON_VERSION": "3.10"
  }
}
